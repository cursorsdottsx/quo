{
    "name": "quo",
    "publisher": "kelsny",
    "displayName": "Quo Language Support",
    "description": "Adds language support for Quo in VSCode.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "quo",
                "aliases": [
                    "Quo",
                    "quo"
                ],
                "extensions": [
                    ".quo"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "dark": "./icons/quo.png",
                    "light": "./icons/quo.png"
                }
            }
        ],
        "grammars": [
            {
                "language": "quo",
                "scopeName": "source.quo",
                "path": "./syntaxes/quo.tmLanguage.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kelsny/quo.git"
    },
    "bugs": {
        "url": "https://github.com/kelsny/quo/issues"
    },
    "homepage": "https://github.com/kelsny/quo#readme"
}
